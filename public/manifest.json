{
  "manifest_version": 2,
  "name": "chrome-wikipedia-plugin",
  "description": "search easily on wikipedia",
  "version": "0.0.1",
  "author": "Naboska",
  "browser_action": {
    "default_title": "Wikipedia plugin",
    "default_icon": "icon.png",
    "default_popup": "popup.html"
  },
  "content_scripts": [
    {
      "matches": [
        "<all_urls>"
      ],
      "js": ["main.ts"]
    }
  ],
  "permissions": ["activeTab"]
}